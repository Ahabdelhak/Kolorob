<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"

    android:layout_height="match_parent">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:paddingLeft="10dp"
        android:paddingTop="2dp"
        android:paddingBottom="4dp"
        android:textAppearance="?android:attr/textAppearanceMedium"        android:text="Doc ID"
        android:textColor="@color/black"
        android:id="@+id/event_cost" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:paddingLeft="10dp"
        android:paddingTop="2dp"
        android:paddingBottom="4dp"
        android:textAppearance="?android:attr/textAppearanceMedium"        android:text="Doc_name"
        android:textColor="@color/black"

        android:id="@+id/playground_cost" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"

        android:textSize="20sp"
        android:paddingLeft="10dp"
        android:paddingTop="2dp"
        android:paddingBottom="4dp"
        android:textAppearance="?android:attr/textAppearanceMedium"        android:text="Doc_fee"
        android:id="@+id/remarks" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"

        android:textSize="20sp"
        android:paddingLeft="10dp"
        android:paddingTop="2dp"
        android:paddingBottom="4dp"
        android:textAppearance="?android:attr/textAppearanceMedium"        android:text="Pharmacy Time"
        android:id="@+id/e_cost_ffp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:paddingLeft="10dp"
        android:paddingTop="2dp"
        android:paddingBottom="4dp"
        android:textAppearance="?android:attr/textAppearanceMedium"        android:text="Pharmacy_degree"
        android:textColor="@color/black"

        android:id="@+id/e_cost_foc" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:paddingLeft="10dp"
        android:paddingTop="2dp"
        android:paddingBottom="4dp"
        android:textAppearance="?android:attr/textAppearanceMedium"        android:text="Pharmacy_lmaf"
        android:textColor="@color/black"

        android:id="@+id/p_c_ffp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"

        android:textSize="20sp"
        android:paddingLeft="10dp"
        android:paddingTop="2dp"
        android:paddingBottom="4dp"
        android:textAppearance="?android:attr/textAppearanceMedium"        android:text="Pharmacy_Mbbs"
        android:id="@+id/p_cost_foc" />

</LinearLayout>
